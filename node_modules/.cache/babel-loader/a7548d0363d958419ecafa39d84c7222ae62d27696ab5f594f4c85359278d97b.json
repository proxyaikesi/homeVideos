.value = (currentVideoIndex.value - 1 + videos.value.length) % videos.value.length;\n      updateVisibleVideos();\n    };\n    function directToMove(index) {\n      const height = fullPageaa.value.clientHeight;\n      const scrollPage = fullPageContainer.value;\n      const scrollHeight = -(index * height) + 'px';\n      scrollPage.style.transform = `translateY(${scrollHeight})`;\n      fullpage.current = index;\n    }\n    return {\n      fullPage,\n      mouseWheelHandle,\n      videos,\n      childRef,\n      handlePlay,\n      child,\n      callChildMethod,\n      fullPageContainer,\n      currentVideoIndex,\n      visibleVideos,\n      scrollDown,\n      scrollUp,\n      directToMove,\n      isMuted,\n      isLooping\n    };\n  }\n};","map":{"version":3,"names":["Video","onBeforeMount","getCurrentInstance","ref","reactive","onUpdated","onMounted","nextTick","defineComponent","watch","provide","axios","components","setup","isMuted","isLooping","fullPageContainer","fullPage","videos","visibleVideos","currentVideoIndex","fetchVideos","response","get","data","item","sort","Math","random","value","updateVisibleVideos","error","console","startIndex","endIndex","length","slice","handlePlay","index","pauseOtherVideos","forEach","video","idx","pause","mouseWheelHandle","event","deltaY","scrollDown","scrollUp","directToMove","height","fullPageaa","clientHeight","scrollPage","scrollHeight","style","transform","fullpage","current","childRef","child","callChildMethod"],"sources":["D:\\vue3\\video\\src\\components\\index.vue"],"sourcesContent":["<!-- <div class=\"h1\" @click=\"one\">点我\r\n    <input type=\"text\">\r\n    <router-link to=\"/register\" active-class=\"active\">皮蛋编程</router-link>\r\n  </div> -->\r\n<!-- <button class=\"main_h\" @click=\"callChildMethod\">调用子组件方法</button> -->\r\n<template>\r\n  <div class=\"box\">\r\n    <div class=\"fullPage\" ref=\"fullPage\">\r\n      <div class=\"fullPageContainer\" ref=\"fullPageContainer\" @wheel=\"mouseWheelHandle\">\r\n  