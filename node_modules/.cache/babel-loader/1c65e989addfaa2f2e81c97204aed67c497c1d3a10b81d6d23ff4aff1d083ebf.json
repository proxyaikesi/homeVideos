{"ast":null,"code":"import Video from './video.vue';\nimport { onBeforeMount, getCurrentInstance, ref, reactive, onUpdated, onMounted, nextTick, defineComponent, watch, provide } from \"vue\";\nimport axios from 'axios';\nexport default {\n  components: {\n    Video\n  },\n  setup() {\n    const isMuted = ref(true);\n    const isLooping = ref(true);\n    const fullPageContainer = ref(null);\n    const fullPage = ref(null);\n    const videos = ref([]);\n    const visibleVideos = ref([]);\n    const currentVideoIndex = ref(0);\n    onBeforeMount(() => {\n      fetchVideos();\n    });\n    const fetchVideos = async () => {\n      try {\n        const response = await axios.get('http://localhost:3000/getVideo');\n        const data = response.data.item.sort(() => 0.5 - Math.random());\n        videos.value = data.map(item => ({\n          ...item,\n          ref: null\n        })); // 初始化 ref 为 null\n        updateVisibleVideos();\n      } catch (error) {\n        console.error('Error fetching videos:', error);\n      }\n    };\n    const updateVisibleVideos = () => {\n      const startIndex = currentVideoIndex.value;\n      const endIndex = startIndex + 5 >= videos.value.length ? videos.value.length : startIndex + 5;\n      visibleVideos.value = videos.value.slice(startIndex, endIndex);\n    };\n    const handlePlay = index => {\n      if (currentVideoIndex.value !== index) {\n        currentVideoIndex.value = index;\n        pauseOtherVideos(index);\n        if (videos.value[index].ref) {\n          videos.value[index].ref.play();\n        }\n      }\n    };\n    const pauseOtherVideos = index => {\n      videos.value.forEach((video, idx) => {\n        if (idx !== index && video.ref && video.ref.pause) {\n          video.ref.pause();\n        }\n      });\n    };\n    const mouseWheelHandle = event => {\n      const deltaY = event.deltaY;\n      if (deltaY > 0) {\n        scrollDown();\n      } else {\n        scrollUp();\n      }\n    };\n    const scrollDown = () => {\n      currentVideoIndex.value = (currentVideoIndex.value + 1) % videos.value.length;\n      updateVisibleVideos();\n      nextTick(() => {\n        if (videos.value[currentVideoIndex.value] && videos.value[currentVideoIndex.value].ref) {\n          videos.value[currentVideoIndex.value].ref.play();\n        }\n      });\n    };\n    const scrollUp = () => {\n      currentVideoIndex.value = (currentVideoIndex.value - 1 + videos.value.length) % videos.value.length;\n      updateVisibleVideos();\n      nextTick(() => {\n        if (videos.value[currentVideoIndex.value] && videos.value[currentVideoIndex.value].ref) {\n          videos.value[currentVideoIndex.value].ref.play();\n        }\n      });\n    };\n    // 提供 isMuted 和 isLooping 给子组件\n    provide('isMuted', true);\n    provide('isLooping', true);\n    return {\n      fullPage,\n      mouseWheelHandle,\n      videos,\n      handlePlay,\n      fullPageContainer,\n      currentVideoIndex,\n      visibleVideos,\n      scrollDown,\n      scrollUp,\n      isMuted,\n      isLooping\n    };\n  }\n};","map":{"version":3,"names":["Video","onBeforeMount","getCurrentInstance","ref","reactive","onUpdated","onMounted","nextTick","defineComponent","watch","provide","axios","components","setup","isMuted","isLooping","fullPageContainer","fullPage","videos","visibleVideos","currentVideoIndex","fetchVideos","response","get","data","item","sort","Math","random","value","map","updateVisibleVideos","error","console","startIndex","endIndex","length","slice","handlePlay","index","pauseOtherVideos","play","forEach","video","idx","pause","mouseWheelHandle","event","deltaY","scrollDown","scrollUp"],"sources":["D:\\vue3\\video\\src\\components\\index.vue"],"sourcesContent":["<template>\r\n  <div class=\"box\">\r\n    <div class=\"fullPage\" ref=\"fullPage\">\r\n      <div class=\"fullPageContainer\" ref=\"fullPageContainer\" @wheel.prevent=\"mouseWheelHandle\">\r\n        <Video v-for=\"(item, index) in visibleVideos\" :key=\"index\" :isPlaying=\"index === currentVideoIndex\"\r\n          :inNum=\"index\" :mp4Url=\"item.file_name\" @play=\"handlePlay(index)\" @pause=\"handlePause(index)\"\r\n          v-model:ref=\"videos[index].ref\" class=\"btn\"></Video>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Video from './video.vue'\r\nimport { onBeforeMount, getCurrentInstance, ref, reactive, onUpdated, onMounted, nextTick, defineComponent, watch, provide } from \"vue\";\r\nimport axios from 'axios'\r\nexport default ({\r\n  components: {\r\n    Video,\r\n  },\r\n  setup() {\r\n    const isMuted = ref(true);\r\n    const isLooping = ref(true)\r\n    const fullPageContainer = ref(null);\r\n    const fullPage = ref(null);\r\n    const videos = ref([]);\r\n    const visibleVideos = ref([]);\r\n    const currentVideoIndex = ref(0);\r\n\r\n\r\n    onBeforeMount(() => {\r\n      fetchVideos();\r\n    });\r\n    const fetchVideos = async () => {\r\n      try {\r\n        const response = await axios.get('http://localhost:3000/getVideo');\r\n        const data = response.data.item.sort(() => 0.5 - Math.random());\r\n        videos.value = data.map(item => ({ ...item, ref: null })); // 初始化 ref 为 null\r\n        updateVisibleVideos();\r\n      } catch (error) {\r\n        console.error('Error fetching videos:', error);\r\n      }\r\n    };\r\n    const updateVisibleVideos = () => {\r\n      const startIndex = currentVideoIndex.value;\r\n      const endIndex = startIndex + 5 >= videos.value.length ? videos.value.length : startIndex + 5;\r\n      visibleVideos.value = videos.value.slice(startIndex, endIndex);\r\n    };\r\n\r\n    const handlePlay = (index) => {\r\n    if (currentVideoIndex.value !== index) {\r\n      currentVideoIndex.value = index;\r\n      pauseOtherVideos(index);\r\n      if (videos.value[index].ref) {\r\n        videos.value[index].ref.play(); \r\n      }\r\n    }\r\n  };\r\n\r\n    const pauseOtherVideos = (index) => {\r\n      videos.value.forEach((video, idx) => {\r\n        if (idx !== index && video.ref && video.ref.pause) {\r\n          video.ref.pause();\r\n        }\r\n      });\r\n    };\r\n\r\n    const mouseWheelHandle = (event) => {\r\n      const deltaY = event.deltaY;\r\n      if (deltaY > 0) {\r\n        scrollDown();\r\n      } else {\r\n        scrollUp();\r\n      }\r\n    };\r\n\r\n    const scrollDown = () => {\r\n      currentVideoIndex.value = (currentVideoIndex.value + 1) % videos.value.length;\r\n      updateVisibleVideos();\r\n      nextTick(() => {\r\n        if (videos.value[currentVideoIndex.value] && videos.value[currentVideoIndex.value].ref) {\r\n          videos.value[currentVideoIndex.value].ref.play();\r\n        }\r\n      });\r\n    };\r\n\r\n    const scrollUp = () => {\r\n      currentVideoIndex.value = (currentVideoIndex.value - 1 + videos.value.length) % videos.value.length;\r\n      updateVisibleVideos();\r\n      nextTick(() => {\r\n        if (videos.value[currentVideoIndex.value] && videos.value[currentVideoIndex.value].ref) {\r\n          videos.value[currentVideoIndex.value].ref.play();\r\n        }\r\n      });\r\n    };\r\n    // 提供 isMuted 和 isLooping 给子组件\r\n    provide('isMuted', true);\r\n    provide('isLooping', true);\r\n    return {\r\n      fullPage, mouseWheelHandle, videos, handlePlay,\r\n      fullPageContainer, currentVideoIndex, visibleVideos, scrollDown, scrollUp, isMuted, isLooping\r\n    }\r\n  },\r\n})\r\n\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\n.h1 {\r\n  color: #fff;\r\n  position: absolute;\r\n  z-index: 9999;\r\n}\r\n\r\n.main_h {\r\n  position: relative;\r\n  font-size: 50px;\r\n  color: #fff;\r\n  z-index: 9999;\r\n}\r\n\r\n.box {\r\n  height: 90vh;\r\n  display: flex;\r\n  flex-direction: column;\r\n  position: relative;\r\n  overflow: hidden;\r\n  background: url('../../public/wallpaper.png');\r\n}\r\n\r\n.fullPage {\r\n  position: absolute;\r\n  left: 0;\r\n  top: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  overflow: hidden;\r\n  bac